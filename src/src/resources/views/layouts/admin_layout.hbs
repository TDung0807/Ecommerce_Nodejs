    <script>

            $('.menu a').on('click', function (e) {
                localStorage.setItem('activeMenu', $(this).attr('href'));

                // Check if the clicked link is inside a collapse submenu
                if ($(this).closest('.collapse.submenu').length > 0) {
                    // Remove the 'collapse' class from the parent collapse submenu
                    $(this).closest('.collapse.submenu').removeClass('show');
                }
            });

            // When the page loads
            $(document).ready(function () {
                // Get the active menu item from local storage
                var activeMenu = localStorage.getItem('activeMenu');

                // If there is an active menu item stored
                if (activeMenu) {
                    // Remove the 'active' class from all menu items
                    $('.menu').find('.active').removeClass('active');

                    // Add the 'active' class to the stored menu item
                    $('.menu a[href="' + activeMenu + '"]').parent().addClass('active');

                    // Check if the stored menu item is inside a collapse submenu
                    if ($('.menu a[href="' + activeMenu + '"]').closest('.collapse.submenu').length > 0) {
                        // Remove the 'collapse' class from the parent collapse submenu
                        $('.menu a[href="' + activeMenu + '"]').closest('.collapse.submenu').removeClass('show');
                    }
                }
            });

    </script>


