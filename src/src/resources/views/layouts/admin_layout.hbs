    {{#if success_msg}}
        <input type="hidden" id="success_msg" value="{{success_msg}}">
        <script>
            window.onload = function() {
                const success_msg = document.getElementById('success_msg').value;
                if (success_msg) {
                    const Toast = Swal.mixin({
                        toast: true,
                        position: 'bottom-start',
                        showConfirmButton: false,
                        timer: 2000,
                        timerProgressBar: true,
                        didOpen: (toast) => {
                            toast.addEventListener('mouseenter', Swal.stopTimer)
                            toast.addEventListener('mouseleave', Swal.resumeTimer)
                        }
                    })
                        
                    Toast.fire({
                        icon: 'success',
                        title: success_msg
                    })
                }
            }
        </script>
    {{/if}}
    {{#if error_msg}}
                <input type="hidden" id="error_msg" value="{{error_msg}}">

                <script>
                    window.onload = function() {
                        const error_msg = document.getElementById('error_msg').value;
                        if (error_msg) {
                            const Toast = Swal.mixin({
                                toast: true,
                                position: 'bottom-start',
                                showConfirmButton: false,
                                timer: 2500,
                                timerProgressBar: true,
                                didOpen: (toast) => {
                                    toast.addEventListener('mouseenter', Swal.stopTimer)
                                    toast.addEventListener('mouseleave', Swal.resumeTimer)
                                }
                            })
                                
                            Toast.fire({
                                icon: 'error',
                                title: error_msg
                            })
                        }
                    }
                </script>
    {{/if}}
